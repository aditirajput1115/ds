#include<iostream>
using namespace std;
#define v 5
void prim(int G[v][v]){
    int selected[v];
    int sum=0;
    int edge=0;
    for(int i=0;i<v;i++){
        selected[i]=false;
        
    }
    selected[0]=true;
    int x,y;
    cout<<"Location:\t Time(in min)\n";
    while(edge<v-1){
        int min=99;
        x=0;
        y=0;
        for(int i=0;i<v;i++){
            if(selected[i]){
                for(int j=0;j<v;j++){
                    if(!selected[j] && G[i][j]){
                        if(min>G[i][j]){
                            min=G[i][j];
                            x=i;
                            y=j;
                        }
                    }
                }
            }
        }
        cout<<x<<"-"<<y<<":"<<G[x][y]<<endl;
        sum+=G[x][y];
        selected[y]=true;
        edge++;
    }
    cout<<"Minimum time in minutes is:"<<sum<<"\tminutes"<<endl;
}
int main(){
    int G[v][v];
    cout<<"Enter travel time matrix(0 for no route)\n";
    for(int i=0;i<v;i++){
        for(int j=0;j<v;j++){
            cin>>G[i][j];
        }
    }
    prim(G);
    return 0;
}
